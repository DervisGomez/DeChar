<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.app.dechar">

    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:name=".modelo.DAOApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".JugarActivity" />
        <activity android:name=".ResultadoActivity" />
        <activity
            android:name=".MisCategoriasActivity"
            android:label="Categorias" />
        <activity android:name=".PalabraCategoriaActivity" />
        <activity android:name=".EditarCategoriaActivity" />
        <activity android:name=".DesarrolladorActivity" />
        <activity
            android:name=".AyudaActivity"
            android:label="@string/title_activity_ayuda"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".TiempoActivity"></activity>
    </application>

</manifest>